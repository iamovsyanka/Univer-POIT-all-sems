@model IEnumerable<DbLib.Phone>

@{
    ViewBag.Title = "Index";
}

@section Header
{
    <header>
        <h2>DB</h2>
    </header>
}

<p>
    @Html.ActionLink("Create New", "Add")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Number)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Number)
            </td>
            <td><p><a href="/Dict2/Update/@item.Id">&#9998; Edit</a></p></td>
            <td><p><a href="/Dict2/Delete/@item.Id">&#10006; Delete</a></p></td>
        </tr>
    }

</table>
@section Footer
{
    <footer>
        <h2>Created by Ovsyanka</h2>
    </footer>
}